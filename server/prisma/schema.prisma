// schema.prisma
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGO_URL")
}

model User {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  username  String?   @unique
  password  String?
  slug     String?
  overview Json?
  workout  Json?
  diet     Json?
  userID   String?
  progress  Json?
  photo     Bytes?
}

